<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uniandes.apo2h.misapuntes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".interfaz.ListaCursosActivity"
            android:label="@string/title_activity_lista_cursos"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".interfaz.AgregarCursoActivity"
            android:label="@string/title_activity_agregar_curso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".interfaz.ListaApuntesActivity"
            android:label="@string/title_activity_lista_apuntes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".interfaz.AgregarApunteActivity"
            android:label="@string/title_activity_agregar_apunte"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".interfaz.ListaContactosActivity"
            android:label="@string/title_activity_lista_contactos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".interfaz.AgregarCompanheroActivity"
            android:label="@string/title_activity_agregar_companhero"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>